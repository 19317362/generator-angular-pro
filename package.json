{
  "name": "generator-angular-pro",
  "version": "2.1.3",
  "description": "Web/mobile Angular project generator for scalable, enterprise-grade applications (TypeScript, Sass, Bootstrap, Ionic, UI Router, Font Awesome, Gettext, Cordova...)",
  "repository": "angular-starter-kit/generator-angular-pro",
  "keywords": [
    "yeoman-generator",
    "angular",
    "angularjs",
    "gulp",
    "cordova",
    "bootstrap",
    "ionic",
    "ios",
    "android",
    "scalable",
    "hybrid",
    "enterprise",
    "component",
    "framework",
    "scaffold",
    "boilerplate",
    "app",
    "mobile",
    "web",
    "pro"
  ],
  "author": "Yohan Lasorsa",
  "license": "MIT",
  "preferGlobal": true,
  "main": "index.js",
  "scripts": {
    "test": "scripts/test-generator.sh",
    "deploy": "scripts/update-starter-kit.sh",
    "postpublish": "PACKAGE_VERSION=$(node -p -e \"require('./package.json').version\") && git tag -a $PACKAGE_VERSION -m '$PACKAGE_VERSION' && git push --tags"
  },
  "dependencies": {
    "chalk": "^1.1.1",
    "lodash": "^4.13.1",
    "node-dir": "^0.1.14",
    "yeoman-generator": "^0.24.1",
    "yosay": "^1.2.0"
